    <meta name="twitter:description" content="Review AI correlates with -11% cycle time, but Code AI shows +16% slower cycles. Analysis of 167,000+ PRs from 101 OSS companies.">
    <meta name="twitter:image" content="https://tformance.com/static/images/og-report.png">

    <!-- Additional SEO -->
    <meta name="author" content="Oleksii Ianchuk">
    <meta name="keywords" content="AI coding tools, GitHub Copilot, software engineering, developer productivity, code review, AI impact, DORA metrics">

    <!-- Modern Stack: Tailwind CSS + Chart.js -->
    <script src="https://cdn.tailwindcss.com"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels@2.2.0"></script>

    <!-- Tailwind Config for Custom Theme -->
    <script>
        tailwind.config = {
            darkMode: ['selector', '[data-theme="dark"]'],
            theme: {
                extend: {
                    colors: {
                        primary: '#F97316',
                        secondary: '#5a9997',
                        success: '#22c55e',
                        warning: '#eab308',
                        error: '#ef4444',
                        purple: '#a855f7',
                        // Dark theme
                        'dark-bg': '#1e1e1e',
                        'dark-card': '#2a2a28',
                        'dark-text': '#ccc9c0',
                        'dark-muted': '#8a8880',
                        'dark-border': '#3c3c3a',
                        // Light theme
                        'light-bg': '#f8f9fa',
                        'light-card': '#ffffff',
                        'light-text': '#1f2937',
                        'light-muted': '#6b7280',
                        'light-border': '#e5e7eb',
                    },
                    fontFamily: {
                        sans: ['Inter', 'system-ui', 'sans-serif'],
                        mono: ['JetBrains Mono', 'monospace'],
                    }
                }
            }
        }
    </script>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=JetBrains+Mono:wght@400;500&display=swap" rel="stylesheet">
